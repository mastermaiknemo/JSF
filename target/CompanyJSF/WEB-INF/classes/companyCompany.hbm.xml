<hibernate-mapping>
  <class name="mein.jsf.CompanyCompany" table="company">
    <id column="id" name="id" type="int">
      <generator class="increment"/>
    </id>
    <property name="name">
      <column name="name"/>
    </property>
    <property name="legalform">
      <column name="legalform"/>
    </property>
      <property name="ceo">
      <column name="ceo"/>
    </property>
      <property name="headquater">
      <column name="headquater"/>
    </property>
      <property name="industry">
      <column name="industry"/>
    </property>
    <property name="website">
        <column name="website"></column>
        </property>
  </class>
